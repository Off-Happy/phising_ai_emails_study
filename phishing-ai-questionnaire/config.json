          "prompt": "This email appears to have been written or edited using AI-based tools.",
          "location": "belowStimulus",
          "type": "slider",
          "options": [
            { "label": "Strongly disagree", "value": 1 },
            { "label": "Disagree", "value": 2 },
            { "label": "Neutral", "value": 3 },
            { "label": "Agree", "value": 4 },
            { "label": "Strongly agree", "value": 5 }
          ],
          "startingValue": 3,
          "spacing": 1,
          "snap": true,
          "withBar": true
