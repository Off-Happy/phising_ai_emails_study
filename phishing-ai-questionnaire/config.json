        {
          "id": "reasoning",
          "prompt": "Optional: Briefly explain your reasoning.",
          "required": false,
          "location": "belowStimulus",
          "type": "longText",
          "placeholder": "Your reasoning (optional)"
        },
        {
          "id": "events",
          "prompt": "Interaction log from the email view",
          "location": "belowStimulus",
          "type": "reactive",
          "hidden": true
        }
      ]
    },
