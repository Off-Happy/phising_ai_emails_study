        {
          "id": "consent",
          "prompt": "Please consent to storing and processing your responses.",
          "required": true,
          "location": "belowStimulus",
          "type": "checkbox",
          "options": [{ "label": "I consent", "value": "agree" }]
        }
      ]
    },
    "personal-information": {
      "type": "questionnaire",
      "instructionLocation": "sidebar",
      "instruction": "### How to answer\n\n- For **dropdowns**, pick the option that fits you best.\n- For **sliders (1–7)**: **1 = low / disagree / never**, **7 = high / agree / very often**.",
      "withSidebar": true,
      "response": [
        {
          "id": "age",
          "prompt": "What is your age range?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Under 18",
            "18-24",
            "25-34",
            "35-44",
            "45-54",
            "55-64",
            "65 or older"
          ]
        },
        {
          "id": "gender",
          "prompt": "What is your gender?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select your gender",
          "options": ["Male", "Female", "Non-binary", "Prefer not to say"]
        },
        {
          "id": "continent",
          "prompt": "On which continent do you live?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Europe",
            "North America",
            "South America",
            "Asia",
            "Africa",
            "Australia/Oceania"
          ]
        },
        {
          "id": "education-level",
          "prompt": "What is your highest level of education achieved?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "High school or equivalent",
            "Associate degree",
            "Bachelor's degree",
            "Master's degree",
