{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/v2.3.1/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Emails in the era of LLMs",
    "version": "pilot",
    "authors": ["Claudiu Veres"],
    "date": "2025-11-10",
    "description": "A study that investigates the effectiveness of AI-generated phishing emails.",
    "organizations": ["University of Groningen"]
  },
  "uiConfig": {
    "contactEmail": "a.veres.1@student.rug.nl",
    "helpTextPath": "phishing-ai-questionnaire/assets/help.md",
    "logoPath": "revisitAssets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": false
  },
  "components": {
    "introduction": {
      "type": "markdown",
      "path": "phishing-ai-questionnaire/assets/introduction.md",
      "withSidebar": false,
      "response": [
        {
          "id": "consent",
          "prompt": "Please consent to storing and processing your responses.",
          "required": true,
          "location": "belowStimulus",
          "type": "checkbox",
          "options": [{ "label": "I consent", "value": "agree" }]
        }
      ]
    },
    "personal-information": {
      "type": "questionnaire",
      "withSidebar": false,
      "response": [
        {
          "id": "age",
          "prompt": "What is your age range?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Under 18",
            "18-24",
            "25-34",
            "35-44",
            "45-54",
            "55-64",
            "65 or older"
          ]
        },
        {
          "id": "gender",
          "prompt": "What is your gender?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select your gender",
          "options": ["Male", "Female", "Non-binary", "Prefer not to say"]
        },
        {
          "id": "continent",
          "prompt": "On which continent do you live?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Europe",
            "North America",
            "South America",
            "Asia",
            "Africa",
            "Australia/Oceania"
          ]
        },
        {
          "id": "education-level",
          "prompt": "What is your highest level of education achieved?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "High school or equivalent",
            "Associate degree",
            "Bachelor's degree",
            "Master's degree",
            "Doctorate or higher"
          ]
        },
        {
          "id": "occupation",
          "prompt": "What is your current occupation status?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Unemployed",
            "Student",
            "Employed",
            "Self-employed",
            "Retired",
            "Prefer not to say"
          ]
        },
        {
          "id": "english-proficiency",
          "prompt": "How would you rate your proficiency in English?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Native or bilingual proficiency",
            "Full professional proficiency",
            "Limited professional proficiency",
            "Elementary proficiency",
            "Beginner"
          ]
        },
        {
          "id": "email-experience-english",
          "prompt": "How often do you use email in English?",
          "required": true,
          "location": "aboveStimulus",
          "type": "slider",
          "options": [
            { "label": "Never", "value": 1 },
            { "label": "Rarely", "value": 2 },
            { "label": "Sometimes", "value": 3 },
            { "label": "Often", "value": 4 },
            { "label": "Very often", "value": 5 }
          ],
          "startingValue": 3,
          "spacing": 1,
          "snap": true,
          "withBar": true
        }
      ]
    },
    "phishing-human-finance": {
      "type": "website",
      "path": "phishing-ai-questionnaire/assets/email-template.html",
      "parameters": {
        "emailDataPath": "phishing-ai-questionnaire/assets/phishing-human-finance.json"
      },
      "withSidebar": true,
      "instructionLocation": "sidebar",
      "instruction": "Please read the email shown on the right carefully. Imagine you received this email in your own inbox. Based on your impressions, reading and interaction with the email, indicate how much you agree or disagree with the statements below.\n\nIf the email is not fully visible, please scroll down to view the entire message.",
      "responseOrder": "fixed",
      "response": [
        {
          "id": "phish-judgment",
          "prompt": "This email raises concerns about its legitimacy.",
          "location": "belowStimulus",
          "type": "slider",
          "options": [
            { "label": "Strongly disagree", "value": 1 },
            { "label": "Disagree", "value": 2 },
            { "label": "Neutral", "value": 3 },
            { "label": "Agree", "value": 4 },
