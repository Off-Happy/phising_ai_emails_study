        {
          "id": "email-sender",
          "prompt": "I always verify the sender's email address before interacting with an email.",
          "required": true,
          "location": "aboveStimulus",
          "type": "slider",
          "options": [
            { "label": "1", "value": 1 },
            { "label": "2", "value": 2 },
