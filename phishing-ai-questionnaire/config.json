      "type": "questionnaire",
      "response": [
        {
          "id": "email-aspects-response",
          "prompt": "Thinking back across all the emails you have seen, which aspects generally influenced your answers? (Select all that apply)",
          "location": "aboveStimulus",
          "type": "checkbox",
          "options": [
            { "label": "The sender’s email address", "value": "sender-email" },
            { "label": "The subject line", "value": "subject-line" },
            {
              "label": "The content of the email message",
              "value": "email-body"
            },
            {
              "label": "The presence or nature of links",
              "value": "email-links"
            },
            {
              "label": "The presence or nature of attachments",
              "value": "email-attachments"
            },
            {
              "label": "The overall design, layout, or formatting",
              "value": "design-formatting"
            }
          ]
        },
        {
          "id": "email-aspects-other",
          "prompt": "Other",
          "required": false,
          "location": "aboveStimulus",
          "type": "shortText",
          "placeholder": "Other (optional)"
        }
      ]
