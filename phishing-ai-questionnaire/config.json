        {
          "id": "email-experience-english",
          "prompt": "How often do you use email in English?",
          "required": true,
          "location": "aboveStimulus",
          "type": "slider",
          "options": [
            { "label": "1", "value": 1 },
            { "label": "2", "value": 2 },
            { "label": "3", "value": 3 },
            { "label": "4", "value": 4 },
            { "label": "5", "value": 5 },
            { "label": "6", "value": 6 },
            { "label": "7", "value": 7 }
          ],
          "startingValue": 4,
          "spacing": 1,
          "snap": true,
          "withBar": true
        }
      ]
    },
