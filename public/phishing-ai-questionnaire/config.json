  "studyMetadata": {
    "title": "Phishing AI Questionnaire",
    "version": "pilot",
    "authors": ["Claudiu Veres"],
    "date": "2025-11-10",
    "description": "A study that investigates the effectiveness of AI-generated phishing emails.",
    "organizations": ["University of Groningen"]
  },
  "uiConfig": {
    "contactEmail": "a.veres.1@student.rug.nl",
    "helpTextPath": "phishing-ai-questionnaire/assets/help.md",
    "logoPath": "revisitAssets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": true
  },
  "components": {
    "introduction": {
      "type": "markdown",
      "path": "phishing-ai-questionnaire/assets/introduction.md",
      "response": [
        {
          "id": "consent",
          "prompt": "Please consent to storing and processing your responses.",
          "required": true,
          "location": "belowStimulus",
          "type": "checkbox",
          "options": [{ "label": "I consent", "value": "agree" }]
        }
      ]
    },
    "personal-information": {
      "type": "questionnaire",
      "instruction": "Please answer each question: for dropdowns, choose the most appropriate option; for sliders, use 1â€“7 (agreement statements: 1=Strongly disagree, 7=Strongly agree; frequency: 1=Never, 7=Very often).",
      "response": [
        {
          "id": "gender",
          "prompt": "What is your gender?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select your gender",
          "options": ["Male", "Female", "Non-binary", "Prefer not to say"]
        },
        {
          "id": "age",
          "prompt": "What is your age range?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Under 18",
            "18-24",
            "25-34",
            "35-44",
            "45-54",
            "55-64",
            "65 or older"
          ]
        },
        {
          "id": "continent",
          "prompt": "On which continent do you live?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Europe",
            "North America",
            "South America",
            "Asia",
            "Africa",
            "Australia/Oceania"
          ]
        },
        {
          "id": "education-level",
          "prompt": "What is your highest level of education achieved?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "High school or equivalent",
            "Associate degree",
            "Bachelor's degree",
            "Master's degree",
            "Doctorate or higher"
          ]
        },
        {
          "id": "english-proficiency",
          "prompt": "How would you rate your proficiency in English?",
          "required": true,
          "location": "aboveStimulus",
          "type": "dropdown",
          "placeholder": "Please select an option",
          "options": [
            "Native or bilingual proficiency",
            "Full professional proficiency",
            "Limited professional proficiency",
            "Elementary proficiency",
            "Beginner"
          ]
        },
        {
          "id": "email-experience-english",
          "prompt": "How often do you use email in English?",
          "required": true,
          "location": "aboveStimulus",
          "type": "slider",
          "options": [
            { "label": "1", "value": 1 },
            { "label": "2", "value": 2 },
            { "label": "3", "value": 3 },
            { "label": "4", "value": 4 },
            { "label": "5", "value": 5 },
            { "label": "6", "value": 6 },
            { "label": "7", "value": 7 }
          ],
          "startingValue": 4,
          "spacing": 1,
          "snap": true,
          "withBar": true
        },
        {
          "id": "email-sender",
          "prompt": "I always verify the sender's email address before interacting with an email.",
          "required": true,
          "location": "aboveStimulus",
          "type": "slider",
          "options": [
            { "label": "1", "value": 1 },
            { "label": "2", "value": 2 },
            { "label": "3", "value": 3 },
            { "label": "4", "value": 4 },
            { "label": "5", "value": 5 },
            { "label": "6", "value": 6 },
            { "label": "7", "value": 7 }
